{
  "name": "rishiqing",
  "version": "2.1.0",
  "electronVersion": "1.0.0",
  "main": "main.js",
  "env": "dev",
  "build": {
    "app-bundle-id": "release.rishiqing.electron"
  },
  "MIN_WINDOW_WIDTH": 1218,
  "MIN_WINDOW_HEIGHT": 620,
  "CompanyName": "北京创仕科锐信息技术有限责任公司",
  "FileDescription": "rishiqing",
  "OriginalFilename": "rishiqing.exe",
  "ProductName": "rishiqing",
  "InternalName": "rishiqing.exe",
  "scripts": {
    "start": "electron main.js",
    "postinstall": "install-app-deps",
    "dist": "build",
    "package-mac-beta": "grunt --env=beta --platform=darwin && node ./package.js --arch=x64 --platform=darwin",
    "package-mac-release": "grunt --env=release --platform=darwin && node ./package.js --arch=x64 --platform=darwin",
    "package-win32-x64-beta": "grunt --env=beta --platform=win32 && node ./package.js --arch=x64 --platform=win32",
    "package-win32-x64-release": "grunt --env=release --platform=win32 && node ./package.js --arch=x64 --platform=win32",
    "package-win32-ia32-beta": "grunt --env=beta --platform=win32 && node ./package.js --arch=ia32 --platform=win32",
    "package-win32-ia32-release": "grunt --env=release --platform=win32 && node ./package.js --arch=ia32 --platform=win32"
  },
  "dependencies": {
    "jquery": "2.2.0",
    "request": "2.72.0",
    "winreg": "0.0.12"
  },
  "devDependencies": {
    "electron-builder": "^3.20.0",
    "electron-packager": "7.0.1",
    "electron-prebuilt": "1.0.0",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compass": "^1.1.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify": "^0.11.1",
    "grunt-zip": "^0.17.1",
    "yargs": "^4.1.0"
  }
}
